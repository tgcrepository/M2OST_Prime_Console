        </small>

        <script>
const searchInput = document.querySelector('#search-input');
const searchIcon = document.querySelector('#search-icon');
const searchBtn = document.querySelector('#search-btn');
const resultDropdown = document.querySelector('#result-dropdown');
const productLink = document.querySelector('#productLink');

let categoryIds = []; // Global variable for category IDs

// Function to toggle the search bar
const toggleSearchBar = (event) => {
    if (searchInput.style.display === 'none') {
        searchInput.style.display = 'inline-block';
        searchBtn.style.display = 'inline-block';
        searchInput.focus();
    } else {
        // Hide the search input and button, clear text, and close dropdown
        searchInput.style.display = 'none';
        searchBtn.style.display = 'none';
        searchInput.value = ''; // Clear the input
        resultDropdown.style.display = 'none'; // Hide the dropdown
    }

    const tabPane = $('.tab-pane.active');
    const hrefCount = tabPane.find('a[href]').length;
    console.log('Number of href elements inside the active tab-pane:', hrefCount);

    categoryIds = [];
    tabPane.find('a[href]').each(function () {
        const href = $(this).attr('href');
        const matches = href.match(/CategoryID=(\d+)/);
        if (matches && matches[1]) {
            const categoryId = matches[1];
            if (!categoryIds.includes(categoryId)) {
                categoryIds.push(categoryId);
            }
        }
    });

    console.log('Unique CategoryIDs:', categoryIds);
    event.preventDefault();
};

// Function to perform search
const performSearch = (event) => {
    event.preventDefault();
    const query = searchInput.value.trim();

    if (query) {
        $.ajax({
            url: '@Url.Action("Searchbar", "Dashboard")',
            type: 'GET',
            data: {
                searchTerm: query,
                categoryIds: categoryIds.join(',')
            },
            success: (response) => {
                debugger;
                resultDropdown.innerHTML = '';

                if (response.length > 0) {
                    const groupedResults = response.reduce((acc, item) => {
                        if (!acc[item.CONTENT_HEADER]) {
                            acc[item.CONTENT_HEADER] = [];
                        }
                        acc[item.CONTENT_HEADER].push(item);
                        return acc;
                    }, {});

                    Object.keys(groupedResults).forEach((header) => {
                        const headerDiv = document.createElement('div');
                        headerDiv.textContent = header;
                        headerDiv.classList.add('dropdown-header');
                        resultDropdown.appendChild(headerDiv);

                        const contentDiv = document.createElement('div');
                        contentDiv.classList.add('dropdown-category');
                        resultDropdown.appendChild(contentDiv);

                        groupedResults[header].forEach((item) => {
                            const subItemDiv = document.createElement('div');
                            subItemDiv.textContent = item.CONTENT_QUESTION;
                            subItemDiv.classList.add('dropdown-item');
                            subItemDiv.setAttribute('data-content-id', item.ID_CONTENT);
                            subItemDiv.addEventListener('click', () => {
                                productLink.href = `@Url.Action("ProductQueAns", "Dashboard")?ContentID=${item.ID_CONTENT}&CategoryID=${item.id_category}`;
                                productLink.click();

                                searchInput.value = ''; // Clear the input
                                resultDropdown.style.display = 'none'; // Hide the dropdown
                            });
                            contentDiv.appendChild(subItemDiv);
                        });

                        headerDiv.addEventListener('click', () => {
                            const isVisible = contentDiv.style.display === 'block';
                            contentDiv.style.display = isVisible ? 'none' : 'block';
                        });
                    });
                } else {
                    const noResults = document.createElement('div');
                    noResults.textContent = 'No results found.';
                    noResults.classList.add('no-results');
                    resultDropdown.appendChild(noResults);
                }

                resultDropdown.style.display = 'block';
            },
            error: (xhr, status, error) => {
                console.error('Error during search:', error);
            }
        });
    } else {
        alert('Please enter a search term.');
    }
};

searchIcon.addEventListener('click', toggleSearchBar);
searchBtn.addEventListener('click', performSearch);

document.addEventListener('click', (event) => {
    if (!event.target.closest('.search-container')) {
        resultDropdown.style.display = 'none';
    }
});

        </script>
