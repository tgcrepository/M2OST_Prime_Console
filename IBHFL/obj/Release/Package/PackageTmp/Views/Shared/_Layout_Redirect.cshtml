                                                        /**/
                                                        /**/

                                                        $.ajax({
                                                            url: url,
                                                            type: "GET",
                                                            datatype: "JSON",
                                                            success: function (data) {
                                                                if (data.READ.length != 0 || data.UNREAD.length != 0) {

                                                                    var curl = "{"
                                                                    //console.log(data.READ)
                                                                    for (var i = 0; i < data.READ.length; i++) {
                                                                        dates += '"' + formatDate(data.READ[i].event_start_datetime) + '": { },'
                                                                        //console.log(dates);

                                                                    }
                                                                    dates = curl + dates;
                                                                    //dates += '}'

                                                                    for (var i = 0; i < data.UNREAD.length; i++) {
                                                                        datesUnread += '"' + formatDate(data.UNREAD[i].event_start_datetime) + '": {},'
                                                                        //console.log(datesUnread);
                                                                    }
                                                                    datesUnread = (datesUnread.substr(0, datesUnread.length - 1));
                                                                    datesUnread += '}'

                                                                    var children = dates.concat(datesUnread);

                                                                    if (data.UNREAD.length == 0) {
                                                                        children = children.substr(0, children.length - 2) + "}";
                                                                    }

                                                                    $(".responsive-calendar").responsiveCalendar({
                                                                        events: JSON.parse(children),
                                                                        onActiveDayClick: function () {

                                                                            datecl = getclickeddate($(this).attr('data-year') + "-" + $(this).attr('data-month') + "-" + $(this).attr('data-day'));

                                                                            location.href = "EventList?datecl=" + datecl + "";

                                                                        },

                                                                    });
                                                                }
                                                                else {
                                                                    $(".responsive-calendar").responsiveCalendar({

                                                                    });
                                                                }
                                                            },

                                                            error: function (error) {
                                                                showErrorMessage(error);
                                                            },

                                                        });
                                                    }
                                                    function getclickeddate(date) {

                                                        var values = date;
                                                        values = values.split("-");
                                                        var year = values[0];
                                                        var month = values[1];
                                                        var day = values[2];
                                                        var s = [day, month, year].join('-')

                                                        return [day, month, year].join('-');

                                                    }

                                                    function formatDate(date) {
                                                        var values = date.substr(0, 10);
                                                        values = values.split("-");
                                                        var day = values[0];
                                                        var month = values[1];
                                                        var year = values[2];
                                                        return [year, month, day].join('-');
                                                    }
