            //});


            BindLanguageDropdown()
            $('.events-dropdown span').click(function () {

                var dropDownList = $(this).parent();
                $(this).parent().find('ul').slideToggle().find('li').click(function () {
                    var v = $(this).attr('data-val');
                    var t = $(this).text();
                    $(this).closest('.events-dropdown').find('.search').attr('value', v);
                    $(this).closest('.events-dropdown').find('.subTextLanguage').text(t);
                });
            });

            $('.events-dropdown .language-container li').click(function () {

                var v = $(this).attr('data-val');
                var t = $(this).text();
                var dropDown = $(this).closest('.events-dropdown');
                dropDown.find('.search').val('');
                dropDown.find('.language-container li').show();
                dropDown.find('.subTextLanguage').attr('data-val', v).text(t);
                dropDown.find('.dropdown-content').slideUp();
            });

            $('.events-dropdown .search').on('input', function () {

                var searchItem = $(this).val().toLowerCase();
                var languageContainer = $(this).closest('.events-dropdown').find('.language-container li');
                var noDataMsg = $(this).closest('.events-dropdown').find('.no-data-msg');
                var matchingItems = 0;
                languageContainer.each(function () {
                    var languageText = $(this).text().toLowerCase();
                    if (languageText.includes(searchItem)) {
                        $(this).show();
                        matchingItems++;
                    } else {
                        console.log('hello');
                        $(this).hide();
                    }
                });
                if (matchingItems === 0) {
                    noDataMsg.show();
                } else {
                    noDataMsg.hide();
                }
            });

            UserPreferenceExistenceCheck();
        });

        function UserPreferenceExistenceCheck() {
            var id_user = @usersession.ID_USER;
            var APIUrl = $('#hdnAPIUrl').val();

            $.ajax({
                url: APIUrl + 'api/Translate/GetIsExistsUserPreference?id_user=' + id_user,
                type: "GET",
                //data: JSON.stringify(oParam),
                cache: false,
                async: false,
                success: function (data) {

                    if (data.KEY == "SUCCESS") {
                        if (data.MESSAGE == "false") {
                            ShowUserLanguagePopup();
                        }
                        else {
                            GetUserPreferredLanguage();
                        }
                    }
                }
            });
        }

        function BindLanguageDropdown() {
            var APIUrl = $('#hdnAPIUrl').val();

            $.ajax({
                url: APIUrl + 'api/Translate/GetAllLanguagesDropdown',
                type: "GET",
                //data: JSON.stringify(oParam),
                cache: false,
                async: false,
                success: function (data) {

                    if (data.KEY == "SUCCESS") {
                        if (data.MESSAGE.length > 0) {
                            var Data = data.MESSAGE;
                            var LanguageData = $.parseJSON(Data);

                            for (var i = 0; i < LanguageData.length; i++) {
                                $('.language-container').append('<li data-val="' + LanguageData[i].id_language + '">' + LanguageData[i].language_name + '</li>');
                            }
                        }
                    }
                }
            });
        }

        function GetUserPreferredLanguage() {
            debugger;
            var id_user = @usersession.ID_USER;
            var APIUrl = $('#hdnAPIUrl').val();

            $.ajax({
                url: APIUrl + 'api/Translate/GetUserLanguagePreference?id_user=' + id_user,
                type: "GET",
                //data: JSON.stringify(oParam),
                cache: false,
                async: false,
                success: function (data) {

                    if (data.KEY == "SUCCESS") {
                        if (data.MESSAGE.length > 0) {
                            var Data = data.MESSAGE;
                            var UserPreferenceData = $.parseJSON(Data);

                            if (UserPreferenceData.length>0) {
                                var Id = UserPreferenceData[0].id_language;
                                var Name = UserPreferenceData[0].language_name;

                                var dropDown = $('.events-dropdown .language-container li').closest('.events-dropdown');
                                dropDown.find('.search').val('');
                                dropDown.find('.language-container li').show();
                                dropDown.find('.subTextLanguage').attr('data-val', Id).text(Name);
                                dropDown.find('.dropdown-content').slideUp();
                            }
                        }
                    }
                }
            });
        }

        function SubmitUserLanguagePreferredLanguage() {
            var id_user = @usersession.ID_USER;
            var APIUrl = $('#hdnAPIUrl').val();

            var dropDown = $('.events-dropdown .language-container li').closest('.events-dropdown');
            var SelectedLanguageID = dropDown.find('.subTextLanguage').attr('data-val');

            $.ajax({
                url: APIUrl + 'api/Translate/InsertUpdateUserPreference?id_user=' + id_user + '&id_language=' + SelectedLanguageID,
                type: "POST",
                //data: JSON.stringify(oParam),
                cache: false,
                async: false,
                success: function (data) {

                    if (data.KEY == "SUCCESS") {
                        alert(data.MESSAGE);
                    }
                    else {
                        alert(data.MESSAGE);
                    }
                    $('#languagePopup').modal('hide');
                }
            });
        }
    </script>

